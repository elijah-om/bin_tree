{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "g++",
    "isShellCommand": true,
    "args": [],
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "_gdb",
            "args": ["-g", "-o", "main_gdb.exe", "main.cpp","baseBinTree.h","baseBinTree.cpp","BinTree.h","BinTree.cpp"]
        },
        {
            "taskName": "compil",
            "args": ["-o", "main_try.exe", "main.cpp","baseBinTree.h","baseBinTree.cpp","BinTree.h","BinTree.cpp"],
            "isBuildCommand": true
        }
    ]
}